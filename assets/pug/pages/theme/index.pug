                                    .light-card.satisfaction-box.progrees-widget
                                      .d-flex.justify-content-between.align-items-center
                                        span.social-content
                                          i.txt-secondary.fa.fa-instagram.me-1
                                          span.f-12.f-w-500.f-light Instagram
