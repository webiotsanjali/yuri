                              span.txt-primary 60% OFF
                          .card-header-right-icon
                            .dropdown
                              button#overviewButton.btn.dropdown-toggle(type='button', data-bs-toggle='dropdown', aria-expanded='false')
                                | Weekly
                              .dropdown-menu.dropdown-menu-end(aria-labelledby='overviewButton')
                                a.dropdown-item(href='#') Monthly
                                a.dropdown-item(href='#') weekly
                                a.dropdown-item(href='#') yearly
                      .card-body.pt-0.transaction-list 
                        ul 
                          li 
                            span.f-w-500.f-light.f-12 Today
                          li 
                            .transaction-content 
                              .transaction-icon.bg-light-primary 
                                svg
                                  use(href='../assets/svg/icon-sprite.svg#bill')
                              .transaction-right-content  
                                h6 Water Bill
                                span.f-light.f-12.f-w-500 Unsuccessfully
              .col-xxl-3
          // Container-fluid Ends
        include ../../components/footer        
