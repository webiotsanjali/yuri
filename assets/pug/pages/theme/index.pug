                                    .light-card.satisfaction-box.progrees-widget
                                      .d-flex.justify-content-between.align-items-center
                                        span.social-content
                                          i.txt-success.fa.fa-twitter.me-1
                                          span.f-12.f-w-500.f-light Twitter 
                                        span.f-12.f-w-500.f-light 30%
                                      .progress.progress-stripe-success.with-light-background.mt-2(style='height: 5px')
                                        .progress-bar-animated.progress-bar-striped(role='progressbar' style='width: 30%' aria-valuenow='10' aria-valuemin='0' aria-valuemax='100')
                                .swiper-pagination
                  .col-xl-6 
                    .card.height-equal.title-line
                      .card-header.card-no-border
                        .header-top
                          h2 Total Profit
                            span.f-light.f-12.d-block.f-w-500 Special Discount 
                              span.txt-primary 60% OFF
                          .card-header-right-icon
                            .dropdown
                              button#overviewButton.btn.dropdown-toggle(type='button', data-bs-toggle='dropdown', aria-expanded='false')
                                | Weekly
                              .dropdown-menu.dropdown-menu-end(aria-labelledby='overviewButton')
                                a.dropdown-item(href='#') Monthly
                                a.dropdown-item(href='#') weekly
                                a.dropdown-item(href='#') yearly
                      .card-body.pt-0 
                        .profit-data 
                          h2 $15,087
                            span.f-light.f-w-500.f-12 (Another 
                              span.txt-primary.me-1 $35,098  
                              | to Goal) 
                        .profit-chart-container 
                          #profit_chart 
                  .col-xl-6 
                    .card.title-line
                      .card-header.card-no-border
                        .header-top
                          div 
                            h2 Earning Reports
                              span.d-block.f-w-500.f-light.f-12 Weekly Earning Overview
                          .card-header-right-icon
                            .dropdown.icon-dropdown
                              button#earning-avg.btn.dropdown-toggle(type='button', data-bs-toggle='dropdown', aria-expanded='false')
