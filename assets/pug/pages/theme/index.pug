                        ul.report-list 
                          li 
                            .light-card.report-icon 
                              svg
                                use(href='../assets/svg/icon-sprite.svg#dollar-square')
                            div 
                              span.f-12.f-w-500.f-light Earning
                              h4.mt-1.f-w-600 $109.90
                                span.badge.badge-light-success.ms-1
                                  i.me-1(data-feather="trending-up")
                                  | 9.5%
                          li 
                            .light-card.report-icon 
                              svg
                                use(href='../assets/svg/icon-sprite.svg#status-up')
                            div 
                              span.f-12.f-w-500.f-light Profit
                              h4.mt-1.f-w-600 $98.50
                                span.badge.badge-light-primary.ms-1
                                  i.me-1(data-feather="trending-up")
                                  | 13.5%
                  .col-xl-6 
                    .card.title-line
                      .card-header.card-no-border
